{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "details": {
      "type": "component",
      "repeatable": true,
      "component": "product.characteristics",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "family": {
      "type": "enumeration",
      "enum": [
        "mDeck",
        "MAM4PRO"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "datasheet": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "shortDescription": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "longDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "markets": {
      "collection": "markets",
      "via": "products"
    },
    "solutions": {
      "via": "products",
      "collection": "services"
    },
    "explore": {
      "model": "explore",
      "via": "products"
    },
    "partNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "productOptions": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "product.product-options"
    }
  }
}
